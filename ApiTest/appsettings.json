{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "TokenLifetimeSeconds": 86400,
  "Kestrel": {
    "Endpoints": {
      "Http": {
        "Url": "http://*:8080"
      },
      "Http2": {
        "Url": "http://*:8090",
        "Protocols": "Http2"
      }
    }
  }
}
